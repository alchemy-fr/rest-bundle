{
  "name": "alchemy/rest-bundle",
  "description": "Simple REST utility bundle",
  "authors": [
    {
      "name": "Thibaud Fabre",
      "email": "thibaud@aztech.io"
    }
  ],
  "autoload": {
    "psr-4": {
      "Alchemy\\Rest\\": "src/Component",
      "Alchemy\\RestBundle\\": "src/Bundle",
      "Alchemy\\RestProvider\\": "src/Provider"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Alchemy\\RestBundle\\Tests\\": "tests/unit/Bundle"
    }
  },
  "license": "MIT",
  "require-dev": {
    "pagerfanta/pagerfanta": "^1.0.3",
    "phpunit/phpunit": "^4.7",
    "symfony/symfony": "~2.6",
    "silex/silex": "^1.0.0",
    "squizlabs/php_codesniffer": "^2.3"
  },
  "require": {
    "league/fractal": "^0.12.0",
    "willdurand/negotiation": "2.0.0-alpha1"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
